process {
    withName: MEMOTE_REPORT_SNAPSHOT {
        errorStrategy = { task.exitStatus in ((130..145) + 104) ? 'retry' : 'ignore' }
        ext.args = '--solver hybrid'
        publishDir = [
            path: { "${params.outdir}/memote_report_snapshot" },
            mode: params.publish_dir_mode,
            pattern: '*.html'
        ]
    }
}
